/*Tag box*/

.tag-r,
.tag-l {
    border: 2px solid rgba(175, 234, 255, 0.5);
    background-color: rgba(58, 203, 255, 0.3);
}

.tag-r:before,
.tag-r:after {
    position: absolute;
    content: "";
    display: block;
    border-width: .2667rem;
    bottom: 1rem;
    left: -.5333rem;
    border-style: dashed solid dashed dashed;
    border-color: transparent rgba(175, 234, 255, 0.5) transparent transparent;
    font-size: 0;
    line-height: 0;
}

.tag-r:after {
    bottom: 1rem;
    left: -.47rem;
    border-color: transparent rgba(58, 203, 255, 0.3) transparent transparent;
    z-index: 8;
}

.tag-l:before,
.tag-l:after {
    position: absolute;
    content: "";
    display: block;
    border-width: .2667rem;
    bottom: 1.5rem;
    right: -.5333rem;
    border-style: dashed dashed dashed solid;
    border-color: transparent transparent transparent rgba(175, 234, 255, 0.5);
    font-size: 0;
    line-height: 0;
}

.tag-l:after {
    bottom: 1.5rem;
    right: -.47rem;
    border-color: transparent transparent transparent rgba(58, 203, 255, 0.3);
    z-index: 9;
}
