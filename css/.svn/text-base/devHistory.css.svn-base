* {
    margin: 0;
    padding: 0;
}

html,body {
    width: 100%;
    height: 110%;
     overflow: auto;
    /* winphone8和android4+ */
    -webkit-overflow-scrolling: touch;
    /* ios5+ */
    z-index: 1;
}

body {
    width: 100%;
    height: 100%;
    background: url(../img/dev-bg.png) no-repeat;
    background-size: cover;
    background-attachment: fixed;
}

div.dev-wrapper {
    width: 100%;
    height: 110%;
    background-color: none;
    overflow: hidden;
    z-index: 2;
}

.abs-mid-hor {
    position: absolute;
    left: 0;
    right: 0;
    margin: auto;
}

.h12 {
    height: 1.2rem;
}

.h15 {
    height: 1.5rem;
}

.h2 {
    height: 2rem;
}

.h22 {
    height: 2.2rem;
}


/*Person*/

div.person {
    width: 1.36rem;
    height: 1.6133rem;
    top: .2667rem;
    background: url(../img/person.png) no-repeat center center;
    background-size: contain;
    z-index: 3;
}


/*Time line*/

div.time-line {
    position: absolute;
    top:.4rem;
    right: 0;
    bottom: 0;
    left: 0;
    margin:  auto;
    width: .5067rem;
    height: 100%;
    z-index: 4;
}

